{
  "name": "@webbee/bamboo-molecules",
  "version": "0.1.1-alpha.1",
  "repository": "git@github.com:webbeetechnologies/bamboo-molecules.git",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.3.0",
  "workspaces": {
    "packages": [
      "example",
      "test-cases"
    ],
    "nohoist": [
      "**/react-native-web",
      "**/react-native-web/**"
    ]
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepare:husky": "husky install",
    "prepare": "bob build",
    "run:example": "yarn --cwd ./example start",
    "run:storybook": "yarn --cwd ./example storybook",
    "bundle": "yarn prepare && mkdir -p dist && npm pack --pack-destination=dist",
    "release": "yarn bundle && yarn publish dist/webbee-bamboo-molecules-$npm_package_version.tgz --access=public"
  },
  "dependencies": {
    "@react-native-aria/overlays": "^0.3.2",
    "@shopify/flash-list": "^1.3.1",
    "@webbee/bamboo-atoms": "^0.0.18",
    "color": "^4.2.3",
    "lodash.get": "^4.4.2",
    "lodash.memoize": "^4.1.2",
    "lodash.omit": "^4.5.0",
    "react-native-document-picker": "^8.1.2",
    "react-native-mask-input": "^1.2.2",
    "react-native-super-grid": "^4.6.1",
    "ts-deepmerge": "^4.0.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/color": "^3.0.3",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.memoize": "^4.1.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/react": "^18.0.20",
    "@types/react-native": "^0.70.0",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.70.1",
    "react-native-builder-bob": "^0.18.3",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.18.9",
    "typescript": "~4.3.5"
  },
  "peerDependencies": {
    "@types/react": "^16.8.0",
    "@types/react-native": "^0.60.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-native": "^0.60.0",
    "react-native-vector-icons": ">9.2.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
